{
  "name": "rustwebui-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run rsbuild dev --config rsbuild.config.dev.ts",
    "build": "bun run ../build-frontend.js",
    "build:incremental": "bun run rsbuild build --config rsbuild.config.ts",
    "preview": "bun run rsbuild preview --config rsbuild.config.dev.ts",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "lint": "bun run biome lint src",
    "lint:fix": "bun run biome lint src --write",
    "format": "bun run biome format src",
    "format:fix": "bun run biome format src --write",
    "check": "bun run biome check src",
    "check:fix": "bun run biome check src --write"
  },
  "dependencies": {
    "winbox": "^0.2.82"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@rsbuild/core": "^1.4.0",
    "typescript": "^5.0.0"
  }
}